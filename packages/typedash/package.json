{
  "name": "@typedash/typedash",
  "version": "1.1.0",
  "description": "Domain agnostic utility functions",
  "sideEffects": false,
  "files": [
    "src",
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "scripts": {
    "prepublish": "yarn build",
    "clean": "rm -rf dist",
    "build": "echo \"Building...\" && yarn clean && run-p build:cjs build:esm && echo \"Done building!\"",
    "build:cjs": "yarn tsc --build ./tsconfig.cjs.json",
    "build:esm": "yarn tsc --build ./tsconfig.json",
    "dev": "onchange -i \"src/**/*.ts\" -- yarn build",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write . !./dist",
    "test": "jest",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@types/lodash": "4.14.177",
    "@types/ramda": "0.27.1",
    "fp-ts": "^2.12.3",
    "fp-ts-std": "0.13.1",
    "io-ts": "2.2.19",
    "lodash": "4.17.21",
    "monocle-ts": "2.3.12",
    "newtype-ts": "0.3.5",
    "ramda": "0.27.1",
    "ramda-adjunct": "2.35.0",
    "spectacles-ts": "^1.0.7",
    "ts-pattern": "4.0.4",
    "zod": "^3.19.1"
  },
  "devDependencies": {
    "@relmify/jest-fp-ts": "^2.0.2",
    "@typedash/eslint-config-typedash": "0.0.1",
    "@typedash/prettier": "0.0.1",
    "@typedash/tsconfig": "0.0.1",
    "@types/jest": "27.0.3",
    "eslint": "8.9.0",
    "jest": "27.3.1",
    "npm-run-all": "4.1.5",
    "onchange": "7.1.0",
    "prettier": "2.5.1",
    "ts-jest": "27.0.7",
    "typescript": "4.2.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
